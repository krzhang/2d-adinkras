	Author = {Engbers, John and Galvin, David},
	Eprint = {1101.0840},
	File = {:home/yan/Documents/Mendeley Desktop/Engbers, Galvin - H-coloring tori.pdf:pdf},
	Journal = {Journal of Combinatorial Theory, Series B},
	Keywords = {adinkras,combinatorics,graph-homomorphisms},
	Mendeley-Tags = {adinkras,combinatorics,graph-homomorphisms},
	Month = jan,
	Pages = {29},
	Title = {{H-coloring tori}},
